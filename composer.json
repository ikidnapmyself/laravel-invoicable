{
    "name": "ikidnapmyself/laravel-invoicable",
    "type": "library",
    "description": "Easy invoice generation using Laravel Eloquent, originally sander-van-hooft/laravel-invoicable",
    "keywords": [
        "Eloquent",
        "invoicable",
        "invoice",
        "Laravel",
        "laravel-invoicable",
        "payments"
    ],
    "homepage": "https://github.com/ikidnapmyself/laravel-invoicable",
    "license": "MIT",
    "authors": [
        {
            "name": "Burak",
            "email": "burak@myself.com",
            "homepage": "http://ikidnapmyself.com",
            "role": "Earthling"
        }
    ],
    "require": {
        "php": "^7.3.12",
        "ext-intl": "*",
        "dompdf/dompdf": "^0.8.0",
        "illuminate/support": "^6.10.0",
        "illuminate/database": "^6.10.0",
        "laravel/framework": "^6.0",
        "nesbot/carbon": "2.31.0",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "graham-campbell/testbench": "5.4.1",
        "mockery/mockery": "^1.0",
        "orchestra/database": "^4.0.0",
        "orchestra/testbench": "4.0.0",
        "phpunit/phpunit" : "~4.0||~5.0||~6.0||~7.0||~8.0",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "IKidnapMyself\\Invoicable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IKidnapMyself\\Invoicable\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "IKidnapMyself\\Invoicable\\InvoicableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
